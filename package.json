{
  "name": "fight-bot",
  "version": "0.0.1",
  "description": "Discord Bot for parsing UFC Events",
  "main": "app.js",
  "author": "William Ford <me@wford.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently --kill-others-on-fail -n \"TypeScript,Node\" -c \"cyan,green\" \"yarn run watch\" \"yarn run nodemon\"",
    "start": "npx env-cmd node ./dist/app.js",
    "nodemon": "npx env-cmd nodemon --inspect ./dist/app.js",
    "watch": "tsc -w",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "register": "npx env-cmd node ./dist/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.31.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "redis": "^4.0.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=17.1",
    "yarn": "^3.0.1"
  },
  "packageManager": "yarn@3.1.1"
}
