{
  "name": "fight-bot",
  "version": "0.0.1",
  "description": "Discord Bot for parsing UFC Events",
  "main": "app.js",
  "author": "William Ford <me@wford.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-docker": "docker build . -t wjford/fight-bot",
    "dev": "concurrently --kill-others-on-fail -n \"TypeScript,Node\" -c \"cyan,green\" \"yarn run watch\" \"yarn run nodemon\"",
    "start": "npx env-cmd node ./dist/app.js",
    "nodemon": "npx env-cmd nodemon --inspect ./dist/app.js",
    "watch": "tsc -w",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "register": "npx env-cmd node ./dist/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.11",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.33.0",
    "discord.js": "13.6.0",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "redis": "^4.1.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=18.2",
    "yarn": "^3.2.1"
  },
  "packageManager": "yarn@3.2.1"
}
