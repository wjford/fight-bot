{
  "name": "fight-bot",
  "version": "0.0.1",
  "description": "Discord Bot for parsing UFC Events",
  "main": "app.js",
  "author": "William Ford <me@wford.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently --kill-others-on-fail -n \"TypeScript,Node\" -c \"cyan,green\" \"yarn run watch\" \"yarn run nodemon\"",
    "start": "npx env-cmd ./dist/app.js",
    "nodemon": "npx env-cmd nodemon --inspect ./dist/app.js",
    "watch": "tsc -w",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12",
    "yarn": "^1.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
